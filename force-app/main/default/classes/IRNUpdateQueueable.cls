public class IRNUpdateQueueable implements Queueable, Database.AllowsCallouts {
    private String orderId;
    private Map<String, String> irnMap; 
    private Integer attempt;
    
    public IRNUpdateQueueable(String orderId, Map<String, String> irnMap, Integer attempt) {
        this.orderId = orderId;
        this.irnMap = irnMap != null ? irnMap : new Map<String, String>();
        this.attempt = attempt != null ? attempt : 1;
    }
    
    public void execute(QueueableContext context) {
        System.debug('--- Entered IRNUpdateQueueable.execute ---');
        System.debug('OrderId: ' + orderId);
        System.debug('IRN Map: ' + irnMap);
        System.debug('Attempt #: ' + attempt);

        try {
            Order updateOrd = new Order(Id = orderId);
            updateOrd.IRN_Generated_DateTime__c = System.now();

            for (String productType : irnMap.keySet()) {
                String irnNumber = irnMap.get(productType);

                if (productType == 'Accessories') {
                    updateOrd.Accessories_IRN_No__c = irnNumber;
                    System.debug('Setting Accessories_IRN_No__c = ' + irnNumber);
                } else if (productType == 'Merchandise') {
                    updateOrd.Merchandise_IRN_No__c = irnNumber;
                    System.debug('Setting Merchandise_IRN_No__c = ' + irnNumber);
                } else if (productType == 'Vehicle') {
                    updateOrd.IRN_No__c = irnNumber;
                    System.debug('Setting IRN_No__c = ' + irnNumber);
                } else {
                    System.debug('Unsupported product type: ' + productType);
                }
            }

            update updateOrd;
            System.debug('Order update successful ==>'+updateOrd);

        } catch (Exception e) {
            System.debug('Exception in IRNUpdateQueueable: ' + e.getMessage());
            System.debug('Stack Trace: ' + e.getStackTraceString());

            if (attempt < 4 && (
                e.getMessage().contains('UNABLE_TO_LOCK_ROW') || 
                e.getMessage().contains('currently being modified'))
            ) {
                System.debug('Retrying IRN update. Attempt #' + (attempt + 1));
                System.enqueueJob(new IRNUpdateQueueable(orderId, irnMap, attempt + 1));
            } else {
                System.debug('Max retries reached or unrecoverable error. Logging exception...');
                ExceptionHelper.createExceptionLog(e);
            }
        }

        System.debug('--- Exiting IRNUpdateQueueable.execute ---');
    }

