public class EnquiryRecordLock {
    
    
    public static void PreventUpdateForEnquiryStage(List<SObject> childRecords) {
        try {
            if (childRecords == null || childRecords.isEmpty()) {
                return;
            }
            String currentProfileName = [SELECT Profile.Name FROM User WHERE Id = :UserInfo.getUserId()].Profile.Name;
            if (currentProfileName == 'System Administrator') {
                return;
            }
            
            Set<Id> OppIds = new Set<Id>();
            
            for (SObject record : childRecords) {
                Id OppId = null;
                
                if (record.getSObjectType().getDescribe().fields.getMap().containsKey('OpportunityId')) {
                    OppId = (Id) record.get('OpportunityId');
                } else if (record.getSObjectType().getDescribe().fields.getMap().containsKey('Opportunity__c')) {
                    OppId = (Id) record.get('Opportunity__c');
                }
                else if (record.getSObjectType().getDescribe().fields.getMap().containsKey('Enquiry__c')) {
                    OppId = (Id) record.get('Enquiry__c');
                }
                
                if (OppId != null) {
                    OppIds.add(OppId);
                }
            }
            
            if (OppIds.isEmpty()) {
                return;
            }
            
            Map<Id, Opportunity> oppMap = new Map<Id, Opportunity>([SELECT Id, StageName FROM Opportunity WHERE Id IN :OppIds AND StageName ='Closed Won']);
            
            for (SObject record : childRecords) {
                Id OppId = null;
 
                if (record.getSObjectType().getDescribe().fields.getMap().containsKey('OpportunityId')) {
                    OppId = (Id) record.get('OpportunityId');
                } else if (record.getSObjectType().getDescribe().fields.getMap().containsKey('Opportunity__c')) {
                    OppId = (Id) record.get('Opportunity__c');
                }else if (record.getSObjectType().getDescribe().fields.getMap().containsKey('Enquiry__c')) {
                    OppId = (Id) record.get('Enquiry__c');
                }
                
                if (OppId != null && oppMap.containsKey(OppId)) {
                    Opportunity op = oppMap.get(OppId);
                    if (op.StageName != null && op.StageName == 'Closed Won') {
                        record.addError('You cannot update or delete this record because the related Enquiry status is "' + op.StageName + '".');
                    }
                } 
            }
        } catch (Exception e) {
            System.debug('Error Message ==> ' + e.getMessage() + ' | Error Line ==> ' + e.getLineNumber());
        }
    }
    
    
