<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Removing_Edit_Access_Once_Ticket_Closed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Status__c, &quot;Closed&quot;),
    ISCHANGED(Status__c),
    $User.ProfileId = &quot;00eF4000000H9GpIAK&quot;
)</errorConditionFormula>
    <errorMessage>A Closed Ticket Cannot Be Edited</errorMessage>
</ValidationRule>
