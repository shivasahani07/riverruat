<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Min_should_be_Lesser_than_Max_quantity</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(Minimum_Quantity__c &lt;  Maximum_Quantity__c)</errorConditionFormula>
    <errorDisplayField>Maximum_Quantity__c</errorDisplayField>
    <errorMessage>Maximum quantity should be greater than Minimum quantity</errorMessage>
</ValidationRule>
