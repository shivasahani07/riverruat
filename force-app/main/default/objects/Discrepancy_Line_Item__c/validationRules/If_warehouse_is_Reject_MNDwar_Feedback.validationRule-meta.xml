<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>If_warehouse_is_Reject_MNDwar_Feedback</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Warehouse_Approval__c, &quot;Rejected&quot;),
    ISBLANK(Feedback_From_Warehosue__c)
)</errorConditionFormula>
    <errorDisplayField>Feedback_From_Warehosue__c</errorDisplayField>
    <errorMessage>&apos;Feedback From Warehosue&apos; is required when Spare Approval is Rejected.</errorMessage>
</ValidationRule>
