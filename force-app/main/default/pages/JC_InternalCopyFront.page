        <style type="text/css">
            @page {
            size: A4 landscape;
            }
            .grid-container {
            font-size:10px;
            font-family: sans-serif;
            }
            .rectangle {
            border: 1px solid #000;
            background-color: #fff;
            padding:1px;
            margin:1px;
            }
            td{
            padding:2px 5px;
            } 
            tr{
             page-break-inside: avoid;
            }
            .option {
